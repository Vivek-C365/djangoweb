{
    "version": 2,
    "builds": [
        {
            "src": "djangoauthapi1/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.12"
            }
        },
        {
            "src":"build_files.sh",
            "use":"@vercel/static-build",
            "config":{
                "distDIR":"staticfiles_build"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "djangoauthapi1/wsgi.py"
        }
    ]
}
